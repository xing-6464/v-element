{
  "name": "x-element",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": ["dist/index.css"],
  "main": "./dist/x-element.umd.cjs",
  "module": "./dist/x-element.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/x-element.js",
      "require": "./dist/umd/x-element.umd.cjs",
      "types": "./dist/types/src/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest -w",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.7.2",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^24.0.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.0.2",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.8.8",
    "vue": "^3.3.4"
  }
}
